{% mergeDiff msg -%}
{
  "url" : "http://hl7.org/fhir/r4/StructureDefinition/patient-animal",
  "extension" : [
    {%if msg.species -%}
    {
      "url" : "species",
      "valueCodeableConcept" : {{msg.species | to_json_string }}
    },
    {% endif -%}
    {%if msg.breed -%}
    {
      "url" : "breed",
      "valueCodeableConcept" : {{msg.breed | to_json_string }}
    },
    {% endif -%}
    {%if msg.genderStatus -%}
    {
      "url" : "genderStatus",
      "valueCodeableConcept" : {{msg.genderStatus | to_json_string }}
    },
    {% endif -%}
  ],
  "genderStatus" : "",
  "breed" : "",
  "species" : ""
}
{% endmergeDiff -%}