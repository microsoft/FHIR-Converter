{% mergeDiff msg -%}
{
  "contained" : [ {{msg.contained | to_array | batch_render : 'Resource', 'msg'}} ],
  "content" : [ 
    {% for c in msg.content %}
      {% if c.pReference %}
        {{c.pReference | to_json_string | default : '""'}},
      {% endif %}
    {% endfor %}
   ]
}
{% endmergeDiff -%}