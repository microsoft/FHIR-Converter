{% mergeDiff msg -%}
{
  "contained" : [ {{msg.contained | to_array | batch_render : 'Resource', 'msg'}} ],
  "outcome" : "",
  {% assign value = msg.outcome.coding[0].code %}
  {% if value == 'complete' or value == 'error' or value == 'partial' -%}
  "outcome" : "{{value}}",
  {% endif -%}
  "requestOrganization" : ""
}
{% endmergeDiff -%}