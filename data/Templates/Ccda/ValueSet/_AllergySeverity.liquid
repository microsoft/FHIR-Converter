{% assign downcaseCode = code | downcase -%}
{% case downcaseCode -%}
    {% when "fatal" -%}
        "severe",
    {% when "moderate to severe" -%}
        "severe",
    {% when "mild to moderate" -%}
        "moderate",
    {% when "mild" -%}
        "mild",
    {% else -%}
        "",
{% endcase -%}