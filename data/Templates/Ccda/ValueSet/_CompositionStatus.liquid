{% case code -%}
    {% when "draft" -%}
        "preliminary",
    {% when "preliminary" -%}
        "preliminary",
    {% when "final" -%}
        "final",
    {% when "completed" -%}
        "final",
    {% when "amended" -%}
        "amended",
    {% when "replaced" -%}
        "amended",
    {% when "entered-in-error" -%}
        "entered-in-error",
    {% when "error" -%}
        "entered-in-error",
    {% else -%}
        "",
{% endcase -%}