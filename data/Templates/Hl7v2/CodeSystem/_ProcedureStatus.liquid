{
    {% if inCode == 'planned  ' -%}
    "code" : "preparation",
    {% elsif inCode == 'arrived' -%}
    "code" : "on-hold",
    {% elsif inCode == 'triaged' -%}
    "code" : "on-hold",
    {% elsif inCode == 'in-progress' -%}
    "code" : "in-progress",
    {% elsif inCode == 'onleave' -%}
    "code" : "stopped"
    {% elsif inCode == 'finished' -%}
    "code" : "completed",
    {% elsif inCode == 'cancelled' -%}
    "code" : "cancelled",
    {% elsif inCode == 'enetered-in error' -%}
    "code" : "enetered-in error",
    {% elsif inCode == 'unknown' -%}
    "code" : "unknown",
    {% else -%}
    "code" : "{{ inCode }}",
    {% endif -%}
}