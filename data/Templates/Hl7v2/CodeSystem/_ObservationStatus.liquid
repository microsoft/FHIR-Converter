    {% elsif inCode == 'I' -%}
    "code" : "unknown"
    {% elsif inCode == 'N' -%}
    "code" : "unknown"
    {% elsif inCode == 'O' -%}
    "code" : "unknown"
    {% elsif inCode == 'P' -%}
    "code" : "preliminary"
    {% elsif inCode == 'R' -%}
    "code" : "unknown"
    {% elsif inCode == 'S' -%}
    "code" : "unknown"
    {% elsif inCode == 'U' -%}
    "code" : "unknown"
    {% elsif inCode == 'V' -%}
    "code" : "unknown"
    {% elsif inCode == 'W' -%}
