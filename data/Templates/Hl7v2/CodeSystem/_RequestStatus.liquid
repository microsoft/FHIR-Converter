{
    {% if inCode == 'A' -%}
    "code" : "active",
    "display" : "Active",
    "system" : "http://hl7.org/fhir/request-status",
    {% elsif inCode == 'E' -%}
    "code" : "entered-in-error",
    "display" : "Entered in Error",
    "system" : "http://hl7.org/fhir/request-status",
    {% elsif inCode == 'P' -%}
    "code" : "on-hold",
    "display" : "On Hold",
    "system" : "http://hl7.org/fhir/request-status",
    {% elsif inCode == 'R' -%}
    "code" : "revoked",
    "display" : "Revoked",
    "system" : "http://hl7.org/fhir/request-status",
    {% else -%}
    "code" : "{{incode}}",
    "display" : "",
    "system" : "",
    {% endif -%}
}