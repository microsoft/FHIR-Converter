{% capture identifiers -%}
    {% if XON.10 -%}
        {{XON.10.Value}},{{XON.6.Value}}
    {% elsif XON.3 -%}
        {{XON.3.Value}},{{XON.6.Value}}
    {% elsif XON -%}
        {{XON.Value}}
    {% elsif CWE.1 and CWE.3 -%}
        {{CWE.1.Value}},{{CWE.3.Value}}
    {% elsif CWE -%}
        {{CWE.Value}}
    {% elsif AUT.2 and AUT.3 -%}
        {{AUT.2.Value}},{{AUT.3.Value}}
    {% elsif HD.1 and HD.2 -%}
        {{HD.1.Value}},{{HD.2.Value}}
    {% elsif HD -%}
        {{HD.Value}}
    {% elsif HDORG.1 and HDORG.2 -%}
        {{HDORG.1}},{{HDORG.2}}
    {% elsif HDORG -%}
        {{HDORG}}
    {% elsif CX.3 -%}
        {{CX.3.Repeats[0].1.Value}},{{CX.3.Repeats[0].4.Value}}
    {% elsif CX -%}
        {{CX.Value}}
    {% elsif CX_IN1.10 -%}
        {{CX_IN1.10.Repeats[0].1.Value}},{{CX_IN1.10.Repeats[0].4.Value}}
