{% capture identifiers -%}
    {% if PL_Field_Value and PL.10 -%}
        {{PL_Field_Value.Value}},{{PL.10.Value}}
    {% elsif PL_Field_Value and PL_Value_Except_PL5_PL9 -%}
        {{PL_Field_Value.Value}},{{PL_Value_Except_PL5_PL9 }}
    {% elsif PL.10 -%}
        {{PL.10.Value}}
    {% elsif PL -%}
        {{PL.Value}}
    {% elsif XON.10 -%}
        {{XON.10.Value}}
    {% elsif XON.3 and XON.6 -%}
        {{XON.3.Value}},{{XON.6.Value}}
    {% elsif XON -%}
        {{XON.Value}}
    {% elsif HD.1 and HD.2 -%}
        {{HD.1.Value}},{{HD.2.Value}}
    {% elsif HD -%}
        {{HD.Value}}
    {% elsif DLD -%}
        {{DLD.Value}}
    {% elsif ACC.11 -%}
        {{ACC.11.Value}}
    {% elsif ACC.3 -%}
        {{ACC.3.Value}}
    {% elsif CWE -%}
        {{CWE.Value}}
    {% elsif XAD -%}
        {{XAD.Value}}
    {% endif -%}

{% endcapture -%}
{{ identifiers | generate_id_input: 'Location', false | generate_uuid }}
