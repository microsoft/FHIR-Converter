{% else -%}
{% capture input -%}{{XON.Value | generate_id_input: 'Location', false}}{% endcapture -%}
{% endif -%}
{% elsif HD -%}
{% if HD.1 and HD.2 -%}
{% capture segment -%}{{HD.1.Value}},{{HD.2.Value}}{% endcapture -%}
