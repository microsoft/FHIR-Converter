{% capture input -%}{{PL.10.Value | generate_id_input: 'Location', false}}{% endcapture -%}
{% else -%}
{% capture input -%}{{PL.Value | generate_id_input: 'Location', false}}{% endcapture -%}
{% endif -%}
{% elsif XON -%}
{% if XON.10 -%}
{% capture input -%}{{XON.10.Value | generate_id_input: 'Location', false}}{% endcapture -%}
{% elsif XON.3 and XON.6 -%}
{% capture segment -%}{{XON.3.Value}},{{XON.6.Value}}{% endcapture -%}
