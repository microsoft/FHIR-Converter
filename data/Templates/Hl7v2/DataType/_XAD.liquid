{% if XAD -%}
    "line":
    [
        "{{ XAD.1.1 }}",
        "{{ XAD.1.2 }}",
        "{{ XAD.1.3 }}",
        "{{ XAD.2.Value }}",
        "{{ XAD.19.Value }}",
    ],
    "city":"{{ XAD.3.Value }}",
    "state":"{{ XAD.4.Value }}",
    "postalCode":"{{ XAD.5.Value }}",
    "country":"{{ XAD.6.Value }}",
    {% if XAD.7.Value == "M" or XAD.7.Value == "SH" -%}
        "type":"{{ XAD.7.Value | get_property: 'CodeSystem/AddressType', 'code' }}",
    {% endif -%}
    {% if XAD.7.Value == "BA" or XAD.7.Value == "BI" or XAD.7.Value == "C" or XAD.7.Value == "B" or XAD.7.Value == "H" or XAD.7.Value == "O" -%}
        "use":"{{ XAD.7.Value | get_property: 'CodeSystem/AddressUse', 'code' }}",
    {% endif -%}
    {% if XAD.9.9 == null -%}
        "district":"{{ XAD.9.2 }}",
    {% endif -%}
    "district":"{{ XAD.9.9 }}",
    "period":
    {
        "start":"{{ XAD.12.1 | format_as_date_time }}",
        "start":"{{ XAD.13.Value | format_as_date_time }}",
        "end":"{{ XAD.12.2 | format_as_date_time }}",
        "end":"{{ XAD.14.Value | format_as_date_time }}",
    },
{% endif -%}
