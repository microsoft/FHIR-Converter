{% assign textstring = "" -%}
{% if CWE -%}
    "identifier":
    [
        {
            "value":"{{ CWE.1.Value }}",
            {% if CWE.3 -%}
            "system":"http://example.com/v2-to-fhir-converter/Identifier/{{CWE.3.Value | replace: ' ', '-'}}",
            {% endif -%}
        },
        {
            "value":"{{ CWE.4.Value }}",
            {% if CWE.6 -%}
            "system":"http://example.com/v2-to-fhir-converter/Identifier/{{CWE.6.Value | replace: ' ', '-'}}",
            {% endif -%}
        },
        {
            "value":"{{ CWE.10.Value }}",
            {% if CWE.12 -%}
            "system":"http://example.com/v2-to-fhir-converter/Identifier/{{CWE.12.Value | replace: ' ', '-'}}",
            {% endif -%}
        },
    ],
    {% capture textstring %}{{ textstring | append: CWE.2.Value | append: ":" | append: CWE.5.Value | append: ":"| append: CWE.9.Value | append: ":"| append: CWE.11.Value  }}{% endcapture -%}
    "name":"{{ textstring | split: ":" | join: "--" }}",
{% endif -%}
