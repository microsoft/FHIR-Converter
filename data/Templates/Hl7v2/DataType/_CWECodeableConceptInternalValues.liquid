                {% if temp%}
                    "system":"{{temp}}",              
                {% endif %}
                "version":"{{ CWE.8 }}",
        },
        {
                "code":"{{ CWE.10 }}",
                "display":"{{ CWE.11 }}",
                {% if CWE.12 %}
                    "system":"http://example.com/v2-to-fhir-converter/CodeSystem/{{CWE.12 | replace: ' ', '-'}}",
                {% endif %}
                {% capture temp %}{{ CWE.12 | get_property: 'CodeSystem/CodeSystemUrl', 'system' }}{% endcapture %}
                {% if temp%}
                    "system":"{{temp}}",              
                {% endif %}
