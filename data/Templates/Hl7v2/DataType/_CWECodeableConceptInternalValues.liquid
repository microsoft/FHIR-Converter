{% if CWE -%}
    "coding":
    [
        {
                "code":"{{ CWE.1 }}",
                "display":"{{ CWE.2 }}",

                {% if CWE.3 %}
                    "system":"http://example.com/v2-to-fhir-converter/CodeSystem/{{CWE.3 | replace: ' ', '-'}}",
                {% endif %}
                {% capture temp %}{{ CWE.3 | get_property: 'CodeSystem/CodeSystemUrl', 'system' }}{% endcapture %}
                {% if temp%}
                    "system":"{{temp}}",              
                {% endif %}
                "version":"{{ CWE.7 }}",
        },
        {
                "code":"{{ CWE.4 }}",
