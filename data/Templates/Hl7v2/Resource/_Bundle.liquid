{
    "fullUrl":"urn:uuid:{{ ID }}",
    "resource":{
        "resourceType": "Bundle",
        "id":"{{ ID }}",
        {% if MSH.7 -%}
        "timestamp":"{{ MSH.7.Value | format_as_date_time }}",
        {% endif -%}
        "identifier":
        {
            "value":"{{ MSH.10.Value }}",
        },
        {% if MSH.10 -%}
        "type":"message",
        {% endif -%}
    },
    "request":{
        "method":"PUT",
        "url":"Bundle/{{ ID }}",
    },
},
