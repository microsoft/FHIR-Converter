  "answerValueSet" : {% include 'Shared/Reference2Canonical' msg : msg.options -%},
  "answerOption" : {{msg.option | to_json_string }},
  "initial" : [
    {% include 'Questionnaire/Initial' msg: msg -%}
  ],
  "item" : [ {{ msg.item | to_array | batch_render: 'Questionnaire/item', 'msg' }} ],
  "initial[x]" : "",
  "option" : "",
  "options" : ""
}
{% enddiff -%}