{
    "resourceType": "Bundle",
    "type": "transaction",
    "entry": [
        {{#with (getFirstSegments msg.v2 'PID' 'PD1' 'PV1')}}
    
            {{>Resources/VXU_V04/Patient.hbs PID=PID NK1=NK1 PD1=PD1 ID=(generateUUID PID)}},
            {{>Resources/VXU_V04/Encounter.hbs PV1=PV1 ID=(generateUUID PV1)}},
            {{>References/Encounter/subject.hbs ID=(generateUUID PV1) REF=(generateUUID PID)}},
    
            {{#with (getSegmentLists ../msg.v2 'NK1')}}
                {{#each NK1 as |NK1Instance|}}
    
                    {{>Resources/VXU_V04/RelatedPerson.hbs NK1=NK1Instance ID=(generateUUID NK1Instance)}},
                    {{>References/RelatedPerson/patient.hbs ID=(generateUUID NK1Instance) REF=(generateUUID ../../PID)}},
    
                {{/each}}
            {{/with}} 	

            {{#with (getSegmentLists ../msg.v2 'ORC')}}
                {{#each ORC as |ORCInstance|}}
    
                    {{>Resources/VXU_V04/Immunization.hbs ORC=ORCInstance ID=(generateUUID ORCInstance)}},
                    {{>References/Immunization/patient.hbs ID=(generateUUID ORCInstance) REF=(generateUUID ../../PID)}},
    
                {{/each}}
            {{/with}} 
    
            {{#with (getSegmentLists ../msg.v2 'RXA')}}
                {{#each RXA as |RXAInstance|}}
    
                    {{>Resources/VXU_V04/Immunization.hbs RXA=RXAInstance ID=(generateUUID RXAInstance)}},
    
                    {{#with (getRelatedSegmentList ../../../msg.v2 'RXA' RXAInstance-1 'OBX')}}
                        {{#each OBX as |OBXInstance|}}
                            {{>Resources/VXU_V04/Observation.hbs OBX=OBXInstance ID=(generateUUID OBXInstance)}},
                            {{>References/Observation/subject.hbs ID=(generateUUID OBXInstance) REF=(generateUUID ../../../../PID)}},
                            {{>References/Observation/partOf.hbs ID=(generateUUID OBXInstance) REF=(generateUUID RXAInstance)}},
                        {{/each}}
                    {{/with}}
    
                {{/each}}
            {{/with}} 
        {{/with}}
    ]
}