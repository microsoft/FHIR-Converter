{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Device",
        "id":"{{ID}}",
        "identifier":
  		[
        	{{#each (toArray deviceEntry.id)}}
            	{{>DataType/Identifier.hbs id=this}},
            {{/each}}
        ],
        "udiCarrier": [
            {"deviceIdentifier": "{{deviceProps.distinctIdentifier}}", "carrierHRF": "{{deviceProps.fullID}}" }
        ],
        "distinctIdentifier": "{{deviceProps.distinctIdentifier}}",
        "manufactureDate": "{{deviceProps.manufactureDate}}",
        "expirationDate": "{{deviceProps.expirationDate}}",
        "lotNumber": "{{deviceProps.logNumber}}",
        "serialNumber": "{{deviceProps.serialNumber}}",
        "type": {
            "coding": [
                {{#each (toArray deviceEntry.participant.participantRole.playingDevice.code) as |code|}}
                {
                    "code": "{{code.code}}",
                    "system": "{{code.codeSystem}}",
                    "display": "{{code.displayName}}"
                }
                {{/each}}
<!--                codeable concept from participant.participant.playingDevice.code-->
            ],
        },
    "request":{
        "method":"PUT",
        "url":"Device/{{ID}}",
    },
},