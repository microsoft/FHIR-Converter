{{#if mapping}}
	{{#with (evaluate mapping inCode=CWE-1)}}
		"code":"{{code}}",
        "display":"{{display}}",
        "system":"{{system}}",
        "version":"{{version}}",
	{{/with}}
{{else}}
    "code":"{{CWE-1}}",
    "display":"{{CWE-2}}",
    "system":"{{CWE-3}}",
    "version":"{{CWE-7}}",
{{/if}}
