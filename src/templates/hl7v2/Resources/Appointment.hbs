{   
    "resource": {
        "resourceType": "Appointment",
        "identifier":[
            { {{>DataType/EIIdentifier.hbs EI=SCH-2}} }
        ],
        {{#if SCH-7}}
        "reasonCode": [
            {
                "coding": [{
                {{! Remove leading&trailing spaces. Replace multiple spaces in the middle with one in the}}
                    "code": "{{replace (replace SCH-7 '^[ \s]+|[ \s]+$' '') '\s{2,}' ' '}}"
                }]
            }
            ]
        {{/if}}
        "status": {{>CodeSystem/AppointmentStatus.hbs STATUS=(toLower SCH-25)}},
        "minutesDuration": {{SCH-9}},
        "comment": "{{NTE-3}}"
        {{>DataType/TQAppointment.hbs TQ=SCH-11}},
        "created": "{{formatAsDateTime PV1-44}}",
        "participant": [
        {{>References/Patient/basicPatient.hbs PID=PID STATUS='accepted'}},
        {{#if AIL}}
            {{>References/Location/appointment.hbs AIL=AIL STATUS='accepted'}}
        {{/if}},
        {{#if AIP}}
            {{>References/Practitioner/appointment.hbs AIP=AIP STATUS='accepted'}}
        {{/if}}
        ]
    },
    "request":{
        "method":"PUT",
        "url":"Appointment/{{ID}}",
    },
}