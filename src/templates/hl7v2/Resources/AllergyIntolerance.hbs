{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "AllergyIntolerance",
        "id":"{{ID}}",
        "type":{{>DataType/_string.hbs mapping="CodeSystem/AllergyType.hbs" inCode=AL1-2 }},
        "code":
        {
            {{>DataType/CWECodeableConcept.hbs CWE=AL1-3}},
        },
        "reaction":
        [
            {
                "manifestation":
                [
                    {
                        {{#each (getFieldRepeats AL1-5)}}
                            "text":"{{this}}",
                        {{/each}}
                    },
                ],
            },
        ],
        "onsetDateTime":"{{formatAsDateTime AL1-6}}",
    },
    "request":{
        "method":"PUT",
        "url":"AllergyIntolerance/{{ID}}",
    },
},
