{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Immunization",
        "id":"{{ID}}",
        "identifier":
        [
            {
                {{>DataType/EIIdentifier.hbs EI=ORC-2}},
                "type":
                {
                    "coding":
                    [
                        {
                            {{#if ORC-2}},
                                "code":"PLAC",
                            {{/if}},
                            {{#if ORC-2}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0203",
                            {{/if}},
                        },
                    ],
                },
            },
            {
                {{>DataType/EIIdentifier.hbs EI=ORC-3}},
                "type":
                {
                    "coding":
                    [
                        {
                            {{#if ORC-3}},
                                "code":"FILL",
                            {{/if}},
                            {{#if ORC-3}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0203",
                            {{/if}},
                        },
                    ],
                },
            },
        ],
        "recorded":"{{formatAsDateTime ORC-9}}",
        "performer":
        [
            {
                "function":
                {
                    "coding":
                    [
                        {
                            {{#if ORC-12}},
                                "code":"OP",
                            {{/if}},
                            {{#if RXA-10}},
                                "code":"AP",
                            {{/if}},
                            {{#if ORC-12}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0443",
                            {{/if}},
                            {{#if RXA-10}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0443",
                            {{/if}},
                        },
                    ],
                },
            },
        ],
        "occurrenceDateTime":"{{formatAsDateTime RXA-3}}",
        "vaccineCode":
        {
            {{>DataType/CWECodeableConcept.hbs CWE=RXA-5}},
        },
        {{#each (getFieldRepeats RXA-15)}}
            "lotNumber":"{{this}}",
        {{/each}}
        {{#each (getFieldRepeats RXA-16)}}
            "expirationDate":"{{addHyphensDate this}}",
        {{/each}}
        "statusReason":
        {
            {{#each (getFieldRepeats RXA-18)}}
                {{>DataType/CWECodeableConcept.hbs CWE=this}},
            {{/each}}
        },
        "reasonCode":
        [
            {{#each (getFieldRepeats RXA-19)}}
                { {{>DataType/CWECodeableConcept.hbs CWE=this}} },
            {{/each}}
        ],
        "status":{{>DataType/_code.hbs mapping="CodeSystem/CompletionStatus.hbs" inCode=RXA-20 }},
        {{#if (eq RXA-21 "D")}},
            "status":"entered-in-error",
        {{/if}},
        "route":
        {
            {{>DataType/CWECodeableConcept.hbs CWE=RXR-1}},
        },
        "site":
        {
            {{>DataType/CWECodeableConcept.hbs CWE=RXR-2}},
        },
    },
    "request":{
        "method":"PUT",
        "url":"Immunization/{{ID}}",
    },
},
